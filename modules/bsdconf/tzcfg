#!/bin/sh
#v10.0.2
globalconf="${workdir}/cbsd.conf";
CBSDMODULE="bsdconf"
MYARG="set"
MYOPTARG="jname"
MYDESC="Install timezone helper"

[ -f ${globalconf} ] || err 1 "no such conf file";
. ${globalconf}
. ${subr}
. ${inventory}
init $*

#defines
_MYDIR=$(dirname `realpath $0`)

### MAIN Route ###

/usr/sbin/tzsetup ${set} > /dev/null 2>&1

[ $? -eq 1 ] && err 1 "No such zone $set"

/usr/sbin/tzsetup -r
