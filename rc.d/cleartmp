#!/bin/sh
#### MAIN ####
. /etc/rc.conf

workdir="${cbsd_workdir}"
globalconf="${workdir}/cbsd.conf";

[ -f "${globalconf}" ] || {
	echo "no globalconf file"
	exit 1
}

. ${globalconf}


case $1 in
    start)
	if [ -d "${tmpdir}" ]; then
	    #echo Cleanup tmp dir...
	    #echo /usr/bin/find $jailtmpdir -type f -delete
	fi
	;;
esac
