#!/bin/sh

. /etc/rc.conf

workdir="${cbsd_workdir}"
globalconf="${workdir}/cbsd.conf";

[ -f "${globalconf}" ] || {
    echo "no globalconf file"
    exit 1
}

. ${globalconf}
. ${subr}

nodemsgd_stop() {

}

nodemsgd_start() {

}

#### MAIN ####
case $1 in
    start) nodemsgd_start ;;
    stop) nodemsgd_stop ;;
esac
