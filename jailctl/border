#!/usr/local/bin/cbsd
#v9.2.0
CBSDMODULE="jail"
MYARG=""
MYOPTARG=""
MYDESC="List jail run order"
EXTHELP="wf_jorder.html"

. ${subr}

init $*

MYORDER=""
SKIP=""

cbsdsql local 'SELECT jname FROM jails WHERE status<2 AND emulator="bhyve" ORDER BY b_order ASC' |while read jname; do
	printf "${jname} "
done

echo

