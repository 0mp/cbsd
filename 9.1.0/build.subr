# route args param for repo, mkbsdstore, csup, buildworld-related command
[ -z "${ver}" ] && ver=`/sbin/sysctl -n kern.osrelease|cut -d "-" -f1`
[ "${stable}" = "1" ] && ver=${ver%%.*}
[ -z "${rev}" ] && rev="head"
[ "${ver}" = "head" ] && ver="10.0"
[ -n "${arch}" ] || arch=`/sbin/sysctl -n hw.machine_arch`
[ -n "${maxjobs}" ] || maxjobs=`sysctl -n hw.ncpu`
[ -n "${clean}" ] || clean=0
