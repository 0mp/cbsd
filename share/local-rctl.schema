# Default SQL scheme for RCTL
MYTABLE="rctl"
MYCOL="dt cputime datasize memoryuse maxproc openfiles vmemoryuse swapuse nthr wallclock pcpu"

dt="dt TIMESTAMP DATE DEFAULT (datetime('now','localtime')) UNIQUE PRIMARY KEY"
cputime="INTEGER"
datasize="INTEGER"
memoryuse="INTEGER"
maxproc="INTEGER"
openfiles="INTEGER"
vmemoryuse="INTEGER"
swapuse="INTEGER"
nthr="INTEGER"
wallclock="INTEGER"
pcpu="INTEGER"

INITDB=""

