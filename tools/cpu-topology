#!/usr/local/bin/cbsd
#v11.1.17
MYARG=""
MYOPTARG=""
MYDESC="Show cpu topology"
CBSDMODULE="sys"

. ${subr}
init $*

. ${strings}
. ${tools}

/sbin/sysctl -qn kern.sched.topology_spec > /tmp/top.$$
trap "/bin/rm -f /tmp/top.$$" HUP INT ABRT BUS TERM EXIT
${miscdir}/dump_cpu_topology /tmp/top.$$ 2>/dev/null

