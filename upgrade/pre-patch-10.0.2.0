#!/bin/sh
#v10.0.2
# Add necessary string in conf

unset workdir

# MAIN
. /etc/rc.conf

[ -z "${cbsd_workdir}" ] && exit

workdir="${cbsd_workdir}"

[ ! -f "${workdir}/cbsd.conf" ] && exit

. "${workdir}/cbsd.conf"

[ ! -f "${etcdir}/buildworld.conf" -o ! -f "${distdir}/etc/buildworld.conf" ] && exit

if ! grep ^SUPPORTED_ARCH ${etcdir}/buildworld.conf >/dev/null 2>&1; then
    grep ^SUPPORTED_ARCH ${distdir}/etc/buildworld.conf >> ${etcdir}/buildworld.conf
fi
